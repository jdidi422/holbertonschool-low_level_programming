#include <stdio.h>

int main() {
    printf("Program started\n");

    // Print the poem "Requiescat" by Oscar Wilde
    printf("Requiescat\n");
    printf("by Oscar Wilde\n\n");

    printf("Tread lightly, she is near\n");
    printf("Under the snow,\n");
    printf("Speak gently, she can hear\n");
    printf("The daisies grow.\n\n");

    printf("All her bright golden hair\n");
    printf("Tarnished with rust,\n");
    printf("She that was young and fair\n");
    printf("Fallen to dust.\n\n");

    printf("Lily-like, white as snow,\n");
    printf("She hardly knew\n");
    printf("She was a woman, so\n");
    printf("Sweetly she grew.\n\n");

    printf("Coffin-board, heavy stone,\n");
    printf("Lie on her breast,\n");
    printf("I vex my heart alone,\n");
    printf("She is at rest.\n\n");

    printf("Peace, Peace, she cannot hear\n");
    printf("Lyre or sonnet,\n");
    printf("All my life's buried here,\n");
    printf("Heap earth upon it.\n");

return 0;
}
